        <xsd:schema xmlns:a="http://prism.evolveum.com/xml/ns/public/annotation-3" xmlns:tns="http://crai.upeu.edu.pe/xml/ns/midpoint/extension/person/v1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://crai.upeu.edu.pe/xml/ns/midpoint/extension/person/v1">
            <xsd:annotation>
                <xsd:appinfo>
                    <a:defaultPrefix>upeuPerson</a:defaultPrefix>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:import namespace="http://prism.evolveum.com/xml/ns/public/annotation-3"/>
            <xsd:complexType name="personDetails">
                <xsd:annotation>
                    <xsd:appinfo>
                        <a:extension xmlns:qn723="http://midpoint.evolveum.com/xml/ns/public/common/common-3" ref="qn723:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="birthDate" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Fecha de Nacimiento</a:displayName>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="gender" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Género</a:displayName>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="nationality" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Nacionalidad</a:displayName>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="system">
                <xsd:annotation>
                    <xsd:appinfo>
                        <a:extension xmlns:qn582="http://midpoint.evolveum.com/xml/ns/public/common/common-3" ref="qn582:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="kohaPatronCategoryId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Categoria de usuario (Koha)</a:displayName>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ojsUserRole" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>ej: "Author", "Reviewer". Multivalor (OJS)</a:displayName>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="identity">
                <xsd:annotation>
                    <xsd:appinfo>
                        <a:extension xmlns:qn434="http://midpoint.evolveum.com/xml/ns/public/common/common-3" ref="qn434:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="dni" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>DNI</a:displayName>
                                <a:displayOrder>100</a:displayOrder>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="universityIdCard" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Carnet Universitario</a:displayName>
                                <a:displayOrder>110</a:displayOrder>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="academic">
                <xsd:annotation>
                    <xsd:appinfo>
                        <a:extension xmlns:qn559="http://midpoint.evolveum.com/xml/ns/public/common/common-3" ref="qn559:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="orcid" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Identificador ORCID</a:displayName>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="scopusId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Identificador de Scopus</a:displayName>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="googleScholarId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Perfil de Google Scholar</a:displayName>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="employment">
                <xsd:annotation>
                    <xsd:appinfo>
                        <a:extension xmlns:qn729="http://midpoint.evolveum.com/xml/ns/public/common/common-3" ref="qn729:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence/>
            </xsd:complexType>
        </xsd:schema>
