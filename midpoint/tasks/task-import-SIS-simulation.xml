    <binding>loose</binding>
    <category>IMPORT</category>
    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/task/import/handler-3</handlerUri>
    <objectRef oid="{{OID_RESOURCE_SIS_CSV}}" type="ResourceType"/>
