<task xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
      oid="f5605b40-c744-42df-bdc6-01e12daf00c5">
    <name>Reconcile SIS Simulation</name>
    <description>Reconciliación del recurso SIS en modo simulación.</description>
    <executionStatus>runnable</executionStatus>
    <binding>loose</binding>
    <activity>
        <work>
            <reconciliation>
                <resourceRef oid="{{OID_RESOURCE_SIS_CSV}}" type="ResourceType"/>
                <kind>account</kind>
                <intent>default</intent>
                <options>
                    <simulationMode>full</simulationMode>
                </options>
            </reconciliation>
        </work>
    </activity>
    <activityState>
        <simulationMode>full</simulationMode>
    </activityState>
</task>
